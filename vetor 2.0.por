programa {
  funcao inicio() {
    inteiro  numero[8],i, soma 
    escreva("Informe 8 números:\n ")
    para(i=0;i<8;i++){
      escreva(i+1,"º número: ")
      leia(numero[i])

    }soma=numero[0]+numero[1]+numero[2]+numero[3]+numero[4]+numero[5]+numero[6]+numero[7]
    escreva("A soma da soma deu: ",soma)
    //se quiser fazer de 2 em 2: i=i+2

  }
}
